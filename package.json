{
  "name": "open-web-box",
  "version": "0.1.0",
  "description": "A React component library with npm and CDN support",
  "repository": {
    "type": "git",
    "url": "https://github.com/parksben/open-web-box.git"
  },
  "main": "dist/npm/index.js",
  "module": "dist/npm/index.js",
  "types": "dist/npm/index.d.ts",
  "unpkg": "dist/unpkg/index.umd.js",
  "jsdelivr": "dist/unpkg/index.umd.js",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:npm && npm run build:unpkg && npm run build:demo",
    "build:npm": "vite build --config vite.config.npm.ts",
    "build:unpkg": "vite build --config vite.config.unpkg.ts",
    "build:demo": "vite build --config vite.config.demo.ts",
    "deploy": "node scripts/deploy.js",
    "preview": "vite preview",
    "clean": "rm -rf dist website node_modules/.vite",
    "prepare": "npm run build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "check": "biome check --write . && npm run build"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "author": "parksben",
  "license": "MIT",
  "dependencies": {
    "@webcontainer/api": "^1.6.1",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "clsx": "^2.1.1",
    "fflate": "^0.8.2",
    "idb-keyval": "^6.2.2",
    "re-resizable": "^6.11.2",
    "react-router-dom": "^7.9.6"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^20.10.5",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "inquirer": "^9.2.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass-embedded": "^1.93.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0"
  }
}
